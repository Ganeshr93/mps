{
  "name": "mps-microservice",
  "version": "0.0.1",
  "description": "Containerized MPS service",
  "scripts": {
    "install": "cd webui && npm install",
    "build": "tsc && (cd webui && npm run build)",
    "compile": "tsc",
    "lint": "eslint . --ext .ts",
    "start": "npm run build && node ./dist/index.js",
    "test": "jest & (cd webui && npm run test)",
    "dev": "set MPS_USE_VAULT=false&& set MPS_LOG_LEVEL=silly&&set MPS_AUTH_ENABLED=false&& npm start",
    "devx": "MPS_USE_VAULT=false MPS_LOG_LEVEL=silly MPS_AUTH_ENABLED=false npm start"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "node-forge": "^0.10.0",
    "node-vault": "^0.9.20",
    "winston": "^3.0.0",
    "rc": "^1.2.8",
    "ws": "^7.4.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^11.13.20",
    "@types/node-forge": "^0.8.4",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "26.6.0",
    "jest-transform-stub": "^2.0.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^8.1.0",
    "typescript": "^3.6.2"
  }
}
